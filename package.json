{
  "name": "voxelpromo",
  "version": "1.0.0",
  "description": "Sistema de monitoramento de ofertas com automação e IA",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"blue,green\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --ignore config.json --ignore '*.log' --ignore 'logs/**' --exec ts-node src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend; npm run build",
    "start": "node dist/server.js",
    "verify-config": "ts-node scripts/verify-config.ts",
    "test-config": "ts-node scripts/test-config-flow.ts",
    "test-aliexpress": "node scripts/test-aliexpress-simple.js",
    "test-mercadolivre": "node scripts/test-mercadolivre-simple.js",
    "test-pagination": "node scripts/test-collect-aliexpress-pagination.js",
    "test-duplicates": "node scripts/test-duplicates-simple.js",
    "collect-aliexpress": "node scripts/collect-aliexpress.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage",
    "init": "node scripts/check-env.js",
    "create-admin": "node scripts/create-admin.js"
  },
  "keywords": [
    "offers",
    "monitoring",
    "affiliate",
    "automation",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.6.2",
    "baileys": "^6.7.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "groq-sdk": "^0.3.0",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "openai": "^4.20.1",
    "playwright": "^1.40.1",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "rss-parser": "^3.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twitter-api-v2": "^1.15.1",
    "whatsapp-web.js": "^1.23.0",
    "winston": "^3.11.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.12",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}